YAAA,IAAA,CAAA,KAAA,cAAA,OAAA,CAAA,IAAA;AAAA,OAAA;AACA;AATA;AAtCA,CAAA","sourcesContent":["<template>\n  <v-navigation-drawer\n    id=\"appDrawer\"\n    :mini-variant.sync=\"mini\"\n    fixed\n    :dark=\"$vuetify.dark\"\n    app\n    v-model=\"drawer\"\n    width=\"260\"\n    >\n    <v-toolbar color=\"primary darken-1\" dark>\n      <img v-bind:src=\"computeLogo\" height=\"36\" alt=\"Vue Material Admin Template\">\n      <v-toolbar-title class=\"ml-0 pl-3\">\n        <span class=\"hidden-sm-and-down\">Vue Material</span>\n      </v-toolbar-title>        \n    </v-toolbar>\n    <vue-perfect-scrollbar class=\"drawer-menu--scroll\" :settings=\"scrollSettings\">\n      <v-list dense expand>\n        <template v-for=\"(item, i) in menus\">\n            <!--group with subitems-->\n            <v-list-group v-if=\"item.items\" :key=\"item.name\" :group=\"item.group\" :prepend-icon=\"item.icon\" no-action=\"no-action\">\n              <v-list-tile slot=\"activator\" ripple=\"ripple\">\n                <v-list-tile-content>\n                  <v-list-tile-title>{{ item.title }}</v-list-tile-title>\n                </v-list-tile-content>\n              </v-list-tile>\n              <template v-for=\"(subItem, i) in item.items\">\n                <!--sub group-->\n                <v-list-group v-if=\"subItem.items\" :key=\"subItem.name\" :group=\"subItem.group\" sub-group=\"sub-group\">\n                  <v-list-tile slot=\"activator\" ripple=\"ripple\">\n                    <v-list-tile-content>\n                      <v-list-tile-title>{{ subItem.title }}</v-list-tile-title>\n                    </v-list-tile-content>\n                  </v-list-tile>\n                  <v-list-tile v-for=\"(grand, i) in subItem.children\" :key=\"i\" :to=\"item.to\"  ripple=\"ripple\">\n                    <v-list-tile-content>\n                      <v-list-tile-title>{{ grand.title }}</v-list-tile-title>\n                    </v-list-tile-content>\n                  </v-list-tile>\n                </v-list-group>\n                <!--child item-->\n                <v-list-tile v-else :key=\"i\" :to=\"subItem.to\" :disabled=\"subItem.disabled\" :target=\"subItem.target\" ripple=\"ripple\">\n                  <v-list-tile-content>\n                    <v-list-tile-title><span>{{ subItem.title }}</span></v-list-tile-title>\n                  </v-list-tile-content>\n                  <v-list-tile-action v-if=\"subItem.action\">\n                    <v-icon :class=\"[subItem.actionClass || 'success--text']\">{{ subItem.action }}</v-icon>\n                  </v-list-tile-action>\n                </v-list-tile>\n              </template>\n            </v-list-group>\n            <v-subheader v-else-if=\"item.header\" :key=\"i\">{{ item.header }}</v-subheader>\n            <v-divider v-else-if=\"item.divider\" :key=\"i\"></v-divider>\n            <!--top-level link-->\n            <v-list-tile v-else :to=\"item.to\"  ripple=\"ripple\" :disabled=\"item.disabled\" :target=\"item.target\" rel=\"noopener\" :key=\"item.name\">\n              <v-list-tile-action v-if=\"item.icon\">\n                <v-icon>{{ item.icon }}</v-icon>\n              </v-list-tile-action>\n              <v-list-tile-content>\n                <v-list-tile-title>{{ item.title }}</v-list-tile-title>\n              </v-list-tile-content>\n              <v-list-tile-action v-if=\"item.subAction\">\n                <v-icon class=\"success--text\">{{ item.subAction }}</v-icon>\n              </v-list-tile-action>\n            </v-list-tile>\n        </template>\n      </v-list>        \n    </vue-perfect-scrollbar>        \n  </v-navigation-drawer>\n</template>\n<script>\nimport menu from '@/api/menu';\nimport VuePerfectScrollbar from 'vue-perfect-scrollbar';\nexport default {\n  name: 'app-drawer',\n  components: {\n    VuePerfectScrollbar,\n  },\n  props: {\n    expanded: {\n      type: Boolean,\n      default: true\n    },\n  },\n  data: () => ({\n    mini: false,\n    drawer: true,\n    menus: menu,\n    scrollSettings: {\n      maxScrollbarLength: 160\n    }    \n  }),\n  computed: {\n    computeGroupActive () {\n      return true;\n    },\n    computeLogo () {\n      return '/static/m.png';\n    },\n\n    sideToolbarColor () {\n      return this.$vuetify.options.extra.sideNav;\n    }    \n  },\n  created () {\n    window.getApp.$on('APP_DRAWER_TOGGLED', () => {\n      this.drawer = (!this.drawer);\n    });\n  },\n  \n\n  methods: {\n    genChildTarget (item, subItem) {\n      if (subItem.href) return;\n      if (subItem.component) {\n        return {\n          name: subItem.component,\n        };\n      }\n      return { name: `${item.group}/${(subItem.name)}` };\n    },\n  }\n};\n</script>\n\n\n<style lang=\"stylus\">\n// @import '../../node_modules/vuetify/src/stylus/settings/_elevations.styl';\n\n#appDrawer\n  overflow: hidden\n  .drawer-menu--scroll\n    height: calc(100vh - 48px)\n    overflow: auto\n\n</style>\n"],"sourceRoot":"src\\components\\admin_client\\layout"}]}